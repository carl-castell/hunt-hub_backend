<!-- users.ejs -->
<h1><%= title %></h1>

<table border="1" cellpadding="8" cellspacing="0">
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
    </tr>
  </thead>
  <tbody>
    <% users.forEach(function(user) { %>
      <tr>
        <td><%= user.firstName %></td>
        <td><%= user.lastName %></td>
      </tr>
    <% }); %>
  </tbody>
</table>

<a href="/">Back to Home</a>
